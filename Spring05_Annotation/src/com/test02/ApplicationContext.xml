<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

	<context:annotation-config />
	
	<bean id="yoon" class="com.test02.Student">
		<property name="name" value="윤용민"></property>
		<property name="addr" value="수원"></property>
		<property name="age" value="27"></property>
	</bean>
	
	<bean id="jeong" class="com.test02.Student">
		<property name="name" value="정형빈"></property>
		<property name="addr" value="역삼"></property>
		<property name="age" value="32"></property>
	</bean>
	
	<bean id="school" class="com.test02.School">
		<property name="grade" value="1"></property>
	</bean>
</beans>
