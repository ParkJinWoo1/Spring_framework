<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="Yoon" class="com.test03.Developer">
		<constructor-arg index="0" value="윤용민" />
		<constructor-arg index="1" value="800" />
		<property name="dept" value="개발팀" />
	</bean>
	
	<bean id="Jeong" class="com.test03.Engineer">
		<constructor-arg index="0" value="정형빈" />
		<constructor-arg index="1" value="800" />
		<property name="dept" value="기술팀" />
	</bean>

</beans>
